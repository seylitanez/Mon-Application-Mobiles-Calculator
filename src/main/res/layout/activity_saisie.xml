<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    tools:context=".Vue.ActivityCalculMoyenneModule">

    <EditText
        android:id="@+id/coef"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:autofillHints=""
        android:background="@drawable/formsaisie"
        android:ems="10"
        android:maxLength="1"
        android:hint="Coef"
        android:elevation="12dp"
        android:inputType="numberDecimal"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.311"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.69"
        tools:ignore="TextContrastCheck" />
    <EditText
        android:id="@+id/credie"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:autofillHints=""
        android:maxLength="1"
        android:background="@drawable/formsaisie"
        android:ems="10"
        android:hint="credit"
        android:elevation="12dp"
        android:inputType="numberDecimal"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.675"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.689"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/calculer"
        android:layout_width="205dp"
        android:layout_height="62dp"
        android:background="@drawable/forme"
        android:backgroundTint="#D1C4E9"
        android:text="calculer"
        android:textColor="#fff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.814"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/moy"
        android:layout_width="133dp"
        android:layout_height="101dp"
        android:background="@drawable/formsaisie"
        android:elevation="12dp"
        android:backgroundTint="@color/white"
        android:ems="10"
        android:padding="30dp"
        android:text="0.0"
        android:maxLength="5"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.974" />

    <EditText
        android:id="@+id/tpc"
        android:layout_width="200dp"
        android:layout_height="76dp"
        android:autofillHints=""
        android:background="@drawable/formsaisie"
        android:ems="10"
        android:hint="tp"
        android:maxLength="5"
        android:elevation="12dp"
        android:inputType="numberDecimal"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.413"
        tools:ignore="TextContrastCheck" />

    <EditText
        android:id="@+id/emdc"
        android:layout_width="210dp"
        android:layout_height="76dp"
        android:autofillHints=""
        android:background="@drawable/formsaisie"
        android:elevation="12dp"
        android:ems="10"
        android:hint="emd"
        android:maxLength="5"
        android:inputType="numberDecimal"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.538"
        tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck" />

    <EditText
        android:id="@+id/tds"
        android:layout_width="200dp"
        android:layout_height="76dp"
        android:autofillHints=""
        android:background="@drawable/formsaisie"
        android:ems="10"
        android:hint="td"
        android:inputType="numberDecimal"
        android:elevation="12dp"
        android:maxLength="5"

        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.285"
        tools:ignore="TextContrastCheck" />

    <EditText
        android:id="@+id/modules"
        android:layout_width="220dp"
        android:layout_height="73dp"
        android:autofillHints=""
        android:background="@drawable/formsaisie"
        android:elevation="12dp"
        android:ems="10"
        android:hint="module"
        android:maxLength="10"

        android:inputType="textPersonName"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.159"
        tools:ignore="TextContrastCheck" />

    <ImageButton
        android:id="@+id/btnms"
        android:layout_width="127dp"
        android:layout_height="90dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/forme"
        android:contentDescription="todo"
        android:scaleType="centerCrop"
        android:src="@drawable/livres"
        android:transitionName="nametrans"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.006" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="243dp"
        android:layout_height="70dp"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="30dp"
        android:elevation="12dp"
        android:text="calcul_moyenne"
        android:textColor="#1A237E"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <com.google.android.material.divider.MaterialDivider
        android:id="@+id/materialDivider"
        android:layout_width="210dp"
        android:layout_height="5dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="56dp"
        android:background="@drawable/clic"
        android:backgroundTint="@color/purple_200"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.divider.MaterialDivider
        android:id="@+id/materialDivider2"
        android:layout_width="100dp"
        android:layout_height="5dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="68dp"
        android:background="@drawable/clic"
        android:backgroundTint="@color/purple_200"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>